<!--pages/pages-list/xmList/xmList.wxml-->
<view class="page">

		<view class="list">


       <view class="box" wx:for="{{xmList}}" wx:key="id">
       
          <view class="detail">
              
              <view class="img">
                   <image src="" mode="aspectFill" > </image>
              </view>
                
                <view class="bottom">
                    <view class="left">
                       
                       <view class="h3">{{item.title}}</view>
                       <view class="time">{{item.time}}</view>

                    </view>
                    <view class="right">
                       <view class="button">查看项目</view>
                    </view>
                </view>
          </view>

       </view>
		</view>

</view>